<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
    <servlet>
        <servlet-name>simpleServlet</servlet-name>
        <servlet-class>com.simple.SimpleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>redirectServlet</servlet-name>
        <servlet-class>com.simple.RedirectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passVariablesServlet</servlet-name>
        <servlet-class>com.simple.PassVariablesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passVariablesJstlServlet</servlet-name>
        <servlet-class>com.simple.PassVariablesJstlServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>simpleServlet</servlet-name>
        <url-pattern>/simple</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>redirectServlet</servlet-name>
        <url-pattern>/redirect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passVariablesServlet</servlet-name>
        <url-pattern>/passVariables</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passVariablesJstlServlet</servlet-name>
        <url-pattern>/passVariablesJstl</url-pattern>
    </servlet-mapping>
 
</web-app>